{
    "name": "chicorycom/bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Assane Sarr",
            "email": "contact@chicorycom.net"
        }
    ],
    "autoload": {
        "files": [
            "src/Foundation/helpers.php",
            "src/helpers.php",
            "src/Collections/helpers.php"
        ],
        "psr-4" : {
            "Boot\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "filp/whoops": "^2.12",
        "illuminate/auth": "^8.62",
        "illuminate/cache": "^8.62",
        "illuminate/config": "^8.62",
        "illuminate/contracts": "^8.62",
        "illuminate/cookie": "^8.62",
        "illuminate/database": "8.37.*",
        "illuminate/encryption": "^8.62",
        "illuminate/hashing": "^8.62",
        "illuminate/log": "^8.62",
        "illuminate/mail": "^8.61",
        "illuminate/notifications": "^8.62",
        "illuminate/queue": "^8.62",
        "illuminate/redis": "^8.62",
        "illuminate/routing": "^8.62",
        "illuminate/testing": "^8.62",
        "illuminate/validation": "8.*",
        "jenssegers/blade": "dev-master",
        "minishlink/web-push": "dev-master",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/cache": "^5.3",
        "symfony/process": "^5.3",
        "symfony/var-dumper": "^5.3",
        "symfony/yaml": "5.4.x-dev",
        "vlucas/phpdotenv": "5.3.*"
    },
    "replace": {
        "boot/console": "self.version"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
