{
    "name": "chicorycom/bootstrap",
    "license": "MIT",
    "authors": [
        {
            "name": "Assane Sarr",
            "email": "contact@chicorycom.net"
        }
    ],
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4" : {
            "Boot\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "filp/whoops": "^2.12",
        "illuminate/database": "8.37.*",
        "illuminate/mail": "^8.61",
        "illuminate/validation": "8.*",
        "intervention/image": "^2.5",
        "jenssegers/blade": "dev-master",
        "minishlink/web-push": "dev-master",
        "php-di/php-di": "^6.0",
        "php-di/slim-bridge": "^3.1.0",
        "slim/csrf": "1.x-dev",
        "slim/psr7": "^1.5",
        "slim/slim": "4.*",
        "swiftmailer/swiftmailer": "^6.2",
        "symfony/process": "^5.3",
        "symfony/var-dumper": "^5.3",
        "symfony/yaml": "5.4.x-dev",
        "vlucas/phpdotenv": "5.3.*",
        "zeuxisoo/slim-whoops": "0.7.x-dev"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
